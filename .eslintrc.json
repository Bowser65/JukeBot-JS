{
    "parserOptions": {
        "ecmaVersion": 9
    },
    "rules": {
        "curly": ["error", "all"],
        "object-curly-spacing": ["error", "always"],
        "space-before-function-paren": ["error", "always"],
        "indent": ["error", 4],
        "prefer-template": ["error"],
        "quotes": ["error", "single"]
    }
}
